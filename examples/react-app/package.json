{
  "name": "react-app-example",
  "version": "1.0.0",
  "description": "React application example for call-structure-ts",
  "private": true,
  "scripts": {
    "build": "tsc",
    "analyze": "call-structure analyze --entry src/App.tsx#App --output analysis/app.json",
    "analyze-hooks": "call-structure analyze --entry \"src/hooks/**\" --format mermaid",
    "analyze-store": "call-structure analyze --entry src/store/index.ts --output analysis/store.json",
    "test-patterns": "call-structure test --spec component-patterns.yaml"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "axios": "^1.3.0",
    "zustand": "^4.3.0",
    "@tanstack/react-query": "^4.24.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}
